<?xml version="1.0" encoding="UTF-8"?>

<routes id="camel" xmlns="http://camel.apache.org/schema/spring">


	<route id="general">
		<from uri="timer:hello?period=5000" />

		<log message="[${routeId}][${exchangeId}][${header.CamelCreatedTimestamp}][1][${header.CamelMessageHistory}][${body}][${header.CamelExceptionCaught}])" loggingLevel="INFO" logName="ioLogger"/>
    
		<to uri="log:cool" />

	</route>

</routes>
